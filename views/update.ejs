<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="style.css">
    <title>Update</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  </head>
  <body>
   <div class="container my-2">
    <div id="message" class="m-2"></div>
    <h2 class = "h2">Edit</h2>

    <form id = "myform">
        <div class="mb-3">
            <label  class="form-label">Name</label>
            <input type="Text"
             name = "name" 
             class="form-control" 
             id="formName">
            
          </div>
        <div class="mb-3">
          <label  class="form-label">Email address</label>
          <input type="email"  name = "email" class="form-control" id="formEmail" >
          
        </div>
        <div class="mb-3">
          <label class="form-label">Phone Number</label>
          <input type="text" name = "phone" class="form-control" id="formNumber">
        </div>
        
        <button type="submit" class="btn btn-primary enableOnInput">
          Update
        </button>
        <button onclick= "history.go(-1)" class = "btn btn-dark">Back</button>
      </form>
   </div>
   <script src="https://code.jquery.com/jquery-3.7.1.js" integrity="sha256-eKhayi8LEQwp4NKxN+CfCh+3qOVUtJn3QNZ0TciWLP4=" crossorigin="anonymous"></script>

   

  </body>
</html>